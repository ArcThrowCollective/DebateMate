{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "migrate:dev": "APP_ENV=dev dotenv -e .env.dev -- npx prisma migrate dev",
    "migrate:prod": "APP_ENV=prod dotenv -e .env.prod -- npx prisma migrate deploy",
    "generate:dev": "APP_ENV=dev dotenv -e .env.dev -- npx prisma generate",
    "generate:prod": "APP_ENV=prod dotenv -e .env.prod -- npx prisma generate",
    "seed:dev": "APP_ENV=dev dotenv -e .env.dev -- npx prisma db seed",
    "seed:prod": "APP_ENV=prod dotenv -e .env.prod -- npx prisma db seed",
    "build": "npx tsc",
    "start": "tsx src/server.ts",
    "lint": "eslint . -f visualstudio --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts",
    "schema": "prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@prisma/client": "^6.2.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/ws": "^8.5.14",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-http": "^1.22.4",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^6.0.3",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.0"
  }
}
